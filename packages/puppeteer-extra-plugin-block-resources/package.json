{
  "name": "puppeteer-extra-plugin-block-resources",
  "version": "2.3.0",
  "description": "Block resources (images, media, etc.) in puppeteer.",
  "main": "index.js",
  "typings": "index.d.ts",
  "repository": "berstend/puppeteer-extra",
  "author": "berstend",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf index.{js,d.ts,js.map}",
    "prebuild": "run-s clean",
    "docs": "node -e 0",
    "lint": "eslint --ext .js .",
    "build": "tsc --module commonjs",
    "test:js": "ava -v",
    "test": "run-p test:js",
    "test-ci": "run-s test"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "puppeteer",
    "puppeteer-extra",
    "puppeteer-extra-plugin",
    "block-resources",
    "datasaver",
    "chrome",
    "headless",
    "pupeteer"
  ],
  "ava": {
    "concurrency": 1,
    "verbose": true,
    "environmentVariables": {
      "TS_NODE_COMPILER_OPTIONS": "{\"module\":\"commonjs\"}"
    },
    "files": [
      "test/*.test.js"
    ],
    "extensions": [
      "js",
      "ts",
      "mts",
      "cts"
    ],
    "require": [
      "ts-node/register"
    ],
    "failFast": true
  },
  "devDependencies": {
    "ava": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "14.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "puppeteer-extra-plugin": "^3.2.0"
  },
  "gitHead": "babb041828cab50c525e0b9aab02d58f73416ef3"
}
